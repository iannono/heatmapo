div id="view_frame" style="background: white; \
  width: #{site.width}px; \
  height: #{site.height}px; position: absolute; z-index: 9;"

  div id="heat_el" style="width: #{site.width}px; height: #{site.height}px; \
    position: absolute; opacity: 0.8; background: #eee; left: 0px; top: 0px; \
    z-index: 999;"

  img src="/heatimg/#{site.img_name}" style="width: #{site.width}px; \
    height: #{site.height}px; position: absolute; left: 0px; top: 0px; \
    z-index: 99;" 
